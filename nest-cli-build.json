{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "entryFile": "main.ts",
  "sourceRoot": "src",
  
  "compilerOptions": {

    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "outDir": "out",
        "extensions": [".ts", ".tsx", ".js", ".jsx"]
      }
    }
  }
}
